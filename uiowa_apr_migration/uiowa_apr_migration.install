<?php


/**
 * Implements hook_uninstall().
 */
function uiowa_apr_migration_disable() {
    $people = Migration::getInstance('APRPeople');
    $appointments = Migration::getInstance('APRPeopleAppointments');

    $result = $people->processRollback();
    $result = $appointments->processRollback();

    $result = Migration::deregisterMigration('APRPeopleAppointments');
    $result = Migration::deregisterMigration('APRPeople');
}