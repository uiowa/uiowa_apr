{#
/**
 * @file
 * Default theme implementation for the APR Profile pseudo-fields.
 *
 * Available variables
 * - data: Array of biography information from pci.info.bio.
 */
#}

{% if data is not empty %}

<div class="field field-name-apr-profile-bio"{{ attributes }}>
  <h2 class="field-header">Biography</h2>
  <div class="field-items">
    {% for key, item in data %}
      <div class="field-item {{ loop.index is odd ? 'odd' : 'even' }}">
        {{ item|raw }}
      </div>
    {% endfor %}
  </div>
</div>

{% endif %}
